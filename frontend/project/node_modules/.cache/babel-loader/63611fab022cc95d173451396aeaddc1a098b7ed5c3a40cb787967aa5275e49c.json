{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: 'LoginForm',\n  data() {\n    return {\n      userId: '',\n      password: ''\n    };\n  },\n  methods: {\n    goToMainPage() {\n      this.$router.push(\"/MainPage\");\n    },\n    goToSignUpPage() {\n      this.$router.push(\"/SignUpPage\");\n    },\n    async loginUser() {\n      if (!this.userId || !this.password) {\n        alert('Please enter both the user ID and password.');\n        return;\n      }\n      try {\n        const response = await axios.post(\"http://localhost:3001/api/login\", {\n          studentID: this.userId,\n          password: this.password\n        });\n        if (response.status === 200) {\n          const {\n            message,\n            user,\n            token\n          } = response.data;\n          alert(message);\n          localStorage.setItem('user', JSON.stringify(user));\n          localStorage.setItem('token', token);\n          this.$router.push(\"/MainPage\");\n        } else {\n          const {\n            error\n          } = response.data;\n          alert(error);\n        }\n      } catch (error) {\n        console.error(\"An error occurred while logging in. Please try again.\", error);\n        alert(\"An error occurred while logging in. Please try again.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","userId","password","methods","goToMainPage","$router","push","goToSignUpPage","loginUser","alert","response","post","studentID","status","message","user","token","localStorage","setItem","JSON","stringify","error","console"],"sources":["C:\\Users\\김혜빈\\Desktop\\Connect\\frontend\\project\\src\\views\\LoginPage.vue"],"sourcesContent":["<template>\n\n  <form class=\"LoginPage\" @submit.prevent=\"loginUser\">\n    <div class=\"logo\">\n      <img\n        src=\"/image/logo.png\"\n        class=\"img\"\n        alt=\"매칭어스 로고\"\n      />\n    </div>\n    <div class=\"int-area\">\n      <label for=\"id\"></label>\n      <input v-model=\"userId\" type=\"text\" name=\"id\" id=\"id\" placeholder=\"USER ID\" />\n      <label for=\"password\"></label>\n      <input v-model=\"password\" type=\"password\" name=\"pwd\" id=\"pwd\" placeholder=\"PASSWORD\" required />\n    </div>\n\n    <div class=\"btn-area\">\n      <button @click=\"loginUser\" type=\"submit\" id=\"login-button\" :disabled=\"!userId || !password\">\n        LOGIN\n      </button>\n    </div>\n\n    <div class=\"caption\">\n      <p>Don't have an account? <a href=\"#\" @click=\"goToSignUpPage\">Sign up</a></p>\n    </div>\n  </form>\n\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: 'LoginForm',\n  data() {\n    return {\n      userId: '',\n      password: '',\n    };\n  },\n  methods: {\n    goToMainPage() {\n      this.$router.push(\"/MainPage\");\n    },\n    goToSignUpPage() {\n      this.$router.push(\"/SignUpPage\");\n    },\n    async loginUser() {\n      if (!this.userId || !this.password) {\n        alert('Please enter both the user ID and password.');\n        return;\n      }\n\n      try {\n        const response = await axios.post(\"http://localhost:3001/api/login\", {\n          studentID: this.userId,\n          password: this.password,\n        });\n\n        if (response.status === 200) {\n          const { message, user, token } = response.data;\n          alert(message);\n          \n          localStorage.setItem('user', JSON.stringify(user));\n          localStorage.setItem('token', token);\n\n          this.$router.push(\"/MainPage\");\n        } else {\n          const { error } = response.data;\n          alert(error);\n        }\n      }  catch (error) {\n        console.error(\"An error occurred while logging in. Please try again.\", error);\n        alert(\"An error occurred while logging in. Please try again.\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\nbody {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  min-height: 100vh;\n  margin: 0;\n  background-color: #f0f0f0;\n  font-family: Arial, sans-serif;\n}\n\n.logo {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.logo img {\n  max-width: 100%;\n  height: auto;\n}\n\n.LoginPage {\n  background-color: white;\n  border-radius: 10px;\n  box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.1);\n  width: 100%;\n  max-width: 400px;\n  padding: 30px;\n  box-sizing: border-box;\n}\n\n.login-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 100vh;\n}\n\n.int-area {\n  margin-bottom: 20px;\n}\n\n.int-area input {\n  width: 100%;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n  font-size: 16px;\n  outline: none;\n}\n\n.btn-area {\n  margin-top: 20px;\n}\n\n.btn-area button {\n  width: 100%;\n  padding: 10px;\n  background-color: #3498db;\n  color: white;\n  font-size: 18px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n\n.btn-area button:disabled {\n  background-color: #ccc;\n  cursor: not-allowed;\n}\n\n.btn-area button:hover {\n  background-color: #2980b9;\n}\n\n.caption {\n  text-align: center;\n}\n\n.caption p {\n  font-size: 14px;\n  color: #888;\n  margin: 0;\n}\n\n.caption a {\n  color: #3498db;\n  text-decoration: none;\n  font-weight: bold;\n}\n</style>\n"],"mappings":";AA+BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,WAAW,CAAC;IAChC,CAAC;IACDC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC,aAAa,CAAC;IAClC,CAAC;IACD,MAAME,SAASA,CAAA,EAAG;MAChB,IAAI,CAAC,IAAI,CAACP,MAAK,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QAClCO,KAAK,CAAC,6CAA6C,CAAC;QACpD;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,iCAAiC,EAAE;UACnEC,SAAS,EAAE,IAAI,CAACX,MAAM;UACtBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QA