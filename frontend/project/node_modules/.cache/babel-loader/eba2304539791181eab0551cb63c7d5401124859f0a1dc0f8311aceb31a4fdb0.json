on: none;\n}\n\n.navbar_right .btn:hover {\n    transform: scale(1.05);\n}\n\n/* 사이드 메뉴 바 */\n.side_nav {\n    width: 200px;\n    height: 100%;\n    position: fixed;\n    background-color:#E0E0E0; /* 회색으로 변경 */\n    padding: 20px;\n}\n\n.basicMenu {\n    list-style: none;\n    padding: 0;\n    margin: 0;\n}\n\n.basicMenu button {\n    background: none;\n    border: none;\n    cursor: pointer;\n    padding: 10px;\n    margin-bottom: 10px;\n    width: 100%;\n    text-align: left;\n    color: white; /* 버튼 텍스트 색상 흰색으로 설정 */\n    font-size: 16px;\n    transition: background-color 0.3s; /* 호버 효과 추가 */\n}\n\n.basicMenu button:hover {\n    background-color:#cccccc; /* 호버시 진한 회색으로 변경 */\n}\n\n.container {\n    display: flex;\n    justify-content: center;\n}\n\n.board-list {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n    margin-top: 100px;\n}\n\n.board-list li {\n    background-color: white;\n    border-radius: 10px;\n    padding: 15px;\n    margin-bottom: 15px;\n    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n    height: 400px;\n    width: 400px;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    flex: 0 0 calc(50% - 20px);\n}\n\n.board-list .btn {\n    margin-top: auto;\n    background-color: #F6E6F6;\n    color: #9370DB; /* 버튼 텍스트 색상 흰색으로 설정 */\n    border-color: #F6E6F6;\n    width: 100%;\n}\n\n.board-list .btn:hover {\n    transform: scale(1.05); \n}\n\n\n.content {\n    display: grid;\n    grid-template-columns: repeat(4, 1fr);\n    gap: 20px; /* spacing between each grid item */\n    padding: 20px;\n    margin: 20px 0;\n\n    /* 추가된 스타일 */\n    justify-items: center;\n    align-items: center;\n    text-align: center;\n}\n\n.post-it {\n    width: 300px;\n    height: 250px;\n    margin: 10px;\n    padding: 10px;\n    box-shadow: 2px 2px 5px #aaa;\n    background-color: #fefabc; /* Default yellow color for post-it */\n    transition: transform 0.2s; /* Animation for hover effect */\n}\n\n.post-it:hover {\n    transform: scale(1.05); /* Slightly enlarge the post-it on hover */\n}\n\n.post-it.male {\n    background-color: #deeeff; /* Blue color for male */\n}\n\n.post-it.female {\n    background-color: #ffe4ee; /* Pink color for female */\n}\n\n#postItsContainer {\n    display: grid;\n    grid-template-columns: repeat(4, 1fr);\n    gap: 20px; /* spacing between each grid item */\n    padding: 20px;\n    margin: 20px 0;\n\n    /* 추가된 스타일 */\n    justify-items: center;\n    align-items: center;\n    text-align: center;\n}\n\n@media (min-width: 300px) {\n    #postItsContainer {\n        grid-template-columns: repeat(1, 1fr);\n    }\n}\n/* For medium screens (e.g., tablets) */\n@media (min-width: 600px) {\n    #postItsContainer {\n        grid-template-columns: repeat(2, 1fr);\n    }\n}\n\n/* For larger screens (e.g., laptops, desktops) */\n@media (min-width: 900px) {\n    #postItsContainer {\n        grid-template-columns: repeat(4, 1fr);\n    }\n}\n@import \"~bootstrap/dist/css/bootstrap.min.css\";\n@import \"~@fortawesome/fontawesome-free/css/all.min.css\";\n</style>"],"mappings":";AA+HA,eAAe;EACbA,OAAO,EAAE;IACHC,2BAA2BA,CAAA,EAAG;MAChC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,0BAA0B,CAAC;IAC/C;EAEJ;AACA,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}