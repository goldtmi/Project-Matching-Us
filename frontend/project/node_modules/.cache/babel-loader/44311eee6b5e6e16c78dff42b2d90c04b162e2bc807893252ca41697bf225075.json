=\"side_nav\">\n            <ul class=\"basicMenu\">\n                <li><button><p><a href=\"my_info.html\" style=\"text-decoration: none; color: white;\">내 정보</a></p></button></li>\n                <li><button><p><a href=\"matching_apply.html\" style=\"text-decoration: none; color: white;\">매칭 신청</a></p></button></li>\n                <li><button><p><a href=\"matching_apply_detail.html\" style=\"text-decoration: none; color: white;\">매칭 현황</a></p></button></li>\n            </ul>\n        </div>\n    </nav>\n\n    <!-- 매칭 현황 -->\n    <div class=\"match-status\">\n      <!-- 매칭 현황의 내용이 동적으로 표시될 영역 -->\n    </div>\n\n    <!-- 매칭 신청 -->\n<div class=\"content\">\n  <form @submit=\"handleSubmit\">\n    <!-- 게시판 선택 체크박스 -->\n    <div class=\"form-check\">\n      <input type=\"radio\" id=\"board1-2\" name=\"boardType\" value=\"1-2인 게시판\" class=\"form-check-input\">\n      <label class=\"form-check-label\" for=\"board1-2\">1-2인 게시판</label>\n    </div>\n    <div class=\"form-check\">\n      <input type=\"radio\" id=\"board3-more\" name=\"boardType\" value=\"3인 이상 게시판\" class=\"form-check-input\">\n      <label class=\"form-check-label\" for=\"board3-more\">3인 이상 게시판</label>\n    </div>\n    <br>\n\n    <label for=\"postTitle\">게시글 제목:</label>\n    <input type=\"text\" id=\"postTitle\" required>\n    <br>\n\n    <label for=\"postContent\">내용:</label>\n    <textarea id=\"postContent\" required></textarea>\n    <br>\n\n    <label for=\"participants\">상세 인원:</label>\n    <input type=\"number\" id=\"participants\" min=\"1\" max=\"10\" placeholder=\"신청자를 포함한 총인원을 기재해주세요.\" required>\n    <br>\n\n    <button type=\"submit\" class=\"apply-btn\">신청하기</button>\n  </form>\n</div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      // 내 게시물, 댓글 및 답댓글에 대한 샘플 데이터 (실제 데이터와 맞게 수정 가능)\n      myPosts: [\n        {\n          title: \"게시글 1\",\n          content: \"내용 1\",\n          comments: [\n            { author: \"사용자1\", comment: \"댓글 1-1\", replies: [{ author: \"사용자2\", reply: \"답댓글 1-1-1\" }] },\n            { author: \"사용자3\", comment: \"댓글 1-2\", replies: [] },\n          ],\n        },\n        {\n          title: \"게시글 2\",\n          content: \"내용 2\",\n          comments: [\n            { author: \"사용자1\", comment: \"댓글 2-1\", replies: [] },\n            { author: \"사용자4\", comment: \"댓글 2-2\", replies: [{ author: \"사용자1\", reply: \"답댓글 2-2-1\" }] },\n          ],\n        },\n      ],\n    };\n  },\n\n  methods: {\n    // 내 게시물 표시하는 함수\n    displayMyPosts() {\n      const matchStatusDiv = document.querySelector('.match-status');\n      matchStatusDiv.innerHTML = '<h3>내 게시물</h3>';\n\n      const myPostsList = document.createElement('ul');\n      this.myPosts.forEach((post) => {\n        const listItem = document.createElement('li');\n        listItem.textContent = post.title;\n        listItem.addEventListener('click', () => this.displayPostDetails(post));\n        myPostsList.appendChild(listItem);\n      });\n\n      matchStatusDiv.appendChild(myPostsList);\n    },\n\n    // 폼 제출 처리하는 새로운 함수\n    handleSubmit(event) {\n      event.preventDefault();\n\n      // 선택된 게시판 유형 (1-2인 게시판 또는 3인 이상 게시판) 가져오기\n      const boardType = document.querySelector('input[name=\"boardType\"]:checked').value;\n\n      // 게시글 제목과 내용 가져오기\n      const postTitle = document.getElementById('postTitle').value;\n      const postContent = document.getElementById('postContent').value;\n\n      // 참가 인원 수 가져오기\n      const participants = parseInt(document.getElementById('participants').value);\n\n      // 필요에 따라 추가 처리 또는 유효성 검사 수행 가능\n\n      // 제출한 데이터 표시 (데이터를 다른 방식으로 수정 가능)\n      alert(`매칭 신청 완료!\\n게시판: ${boardType}\\n제목: ${postTitle}\\n내용: ${postContent}\\n상세 인원: ${participants}명`);\n    },\n  },\n\n  mounted() {\n    // 페이지 로딩 시 초기 표시 (기본적으로 '내 게시물' 표시)\n    this.displayMyPosts();\n  },\n};\n</script>\n\n\n<style scoped>\nbody {\n    margin: 0;\n    background-color: white;\n}\n\n.MatchingUs {\n    font-size: 28px;\n    color: #9370DB;\n}\n\n.navbar {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    background-color: #F6E6F6;\n    padding: 8px 12px;\n}\n\n.navbar_logo {\n    font-size: 24px;\n}\n\n.navbar_logo .logo_image {\n    margin-right: 8px; \n}\n\n.navbar_welcome {\n    display: flex;\n    align-items: center;\n    margin-right: 30px; \n    margin-top: 10px;\n    font-weight: 400;\n}\n\n.navbar_welcome .welcome {\n    color: #9370DB;\n    margin-bottom: 10px;\n}\n\n.btn.btn-outline-success {\n    /* color: #9370DB; */\n    border-color: #9370DB; \n    font-weight: 500;\n}\n\n.navbar_right {\n    display: flex;\n    align-items: center;\n}\n\n.navbar_right .btn {\n    margin-left: auto;\n    background-color: #F6E6F6; /* Set the background color to match the navbar background color */\n/* Set the border color to match the text color */\n}\n\n.navbar_right .btn a {\n/* Set the text color of the button */\n    text-decoration: none;\n}\n\n.navbar_right .btn:hover {\n    transform: scale(1.05);\n}\n\n\n\n/* 사이드 메뉴 바 */\n.side_nav {\n    width: 200px;\n    height: 100%;\n    position: fixed;\n    background-color:#E0E0E0; /* 회색으로 변경 */\n    padding: 20px;\n}\n\n.basicMenu {\n    list-style: none;\n    padding: 0;\n    margin: 0;\n}\n\n.basicMenu button {\n    background: none;\n    border: none;\n    cursor: pointer;\n    padding: 10px;\n    margin-bottom: 10px;\n    width: 100%;\n    text-align: left;\n    color: white; /* 버튼 텍스트 색상 흰색으로 설정 */\n    font-size: 16px;\n    transition: background-color 0.3s; /* 호버 효과 추가 */\n}\n\n.basicMenu button:hover {\n    background-color:#cccccc; /* 호버시 진한 회색으로 변경 */\n}\n\n.container {\n    display: flex;\n    justify-content: center;\n}\n\n.board-list {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n    margin-top: 100px;\n}\n\n.board-list li {\n    background-color: white;\n    border-radius: 10px;\n    padding: 15px;\n    margin-bottom: 15px;\n    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n    height: 400px;\n    width: 400px;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    flex: 0 0 calc(50% - 20px);\n}\n\n.board-list .btn {\n    margin-top: auto;\n    background-color: #F6E6F6;\n    color: #9370DB; /* 버튼 텍스트 색상 흰색으로 설정 */\n    border-color: #F6E6F6;\n    width: 100%;\n}\n\n.board-list .btn:hover {\n    transform: scale(1.05); \n}\n\nform {\n    max-width: 400px;\n    margin: 0 auto;\n}\n\n.content {\n    max-width: 400px; \n    margin: -60px auto; /* Adjust this value to move the form vertically */\n    padding: 30px;\n    border: 1px solid #ccc;\n    border-radius: 10px;\n    background-color: #fff;\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n}\n\nlabel {\n    display: block;\n    margin-bottom: 8px;\n    font-weight: bold;\n}\n\ninput[type=\"text\"],\ninput[type=\"password\"],\ninput[type=\"email\"],\nselect {\n    width: 100%;\n    margin-bottom: 15px;\n    padding: 12px;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    outline: none;\n    font-size: 16px;\n}\n\ninput[type=\"radio\"] {\n    margin-right: 5px;\n}\n\ninput[type=\"number\"] {\n    width: 100%;\n    margin-bottom: 15px;\n    padding: 12px;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    outline: none;\n    font-size: 16px;\n}\n\ntextarea {\n    width: 100%;\n    margin-bottom: 15px;\n    padding: 12px;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    outline: none;\n    font-size: 16px;\n}\n\nbutton[type=\"submit\"] {\n    width: 100%;\n    background-color: #007BFF;\n    color: white;\n    padding: 10px 15px;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n    font-weight: bold;\n}\n\nh2 {\n    font-size: 24px;\n    margin-bottom: 20px;\n    color: #9370DB;\n}\n\n.apply-btn {\n    background-color: #9370DB;\n    color: white;\n    padding: 12px 20px;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n    font-size: 16px;\n    font-weight: bold;\n    margin-top: 20px;\n}\n\n.apply-btn:hover {\n    background-color: #7B68EE;\n}\n\n@import \"~bootstrap/dist/css/bootstrap.min.css\";\n@import \"~@fortawesome/fontawesome-free/css/all.min.css\";\n</style> "],"mappings":";;gEAGIA,mBAAA,CAcM;EAdDC,KAAK,EAAC;AAAyB,I,aAChCD,mBAAA,CAYM;EAZDC,KAAK,EAAC;AAAiB,I,aACxBD,mBAAA,CAEM;EAFDC,KAAK,EAAC;AAAa,I,aACpBD,mBAAA,CAA2C;EAArCC,KAAK,EAAC;AAAY,GAAC,aAAW,E,gBAExCD,mBAAA,CAOM;EAPDC,KAAK,EAAC;AAAc,I,aACrBD,mBAAA,CAEM;EAFDC,KAAK,EAAC;AAAgB,I,aACvBD,mBAAA,CAA8G;EAAzGC,KAAK,EAAC;AAAS,I,8BAAC,SAAO,G,aAAAD,mBAAA,CAAiC;EAAzBC,KAAK,EAAC;AAAM,GAAC,KAAG,G,8BAAS,KAAG,G,aAAAD,mBAAA,CAAwC;EAArCC,KAAK,EAAC;AAA0B,G,kBAEvGD,mBAAA,CAEO;EAFDC,KAAK,EAAC,QAAQ;EAACC,IAAI,EAAC;iBACtBF,mBAAA,CAA6L;EAArLC,KAAK,EAAC,yBAAyB;EAACE,IAAI,EAAC;iBAASH,mBAAA,CAA8H;EAA3HI,IAAI,EAAC,YAAY;EAACC,KAA8C,EAA9C;IAAA;IAAA;EAAA;iBAA+CL,mBAAA,CAA8C;EAA3CC,KAAK,EAAC;AAAgC,I,8BAAK,UAAQ,E;;gEAkBhMD,mBAAA,CAEM;EAFDC,KAAK,EAAC;AAAc,I,aACvBK,mBAAA,4BAA+B,C;;EAIhCL,KAAK,EAAC;AAAS;;;uBAnClBM,mBAAA,CA+DM,cA9DJD,mBAAA,aAAgB,EAChBE,UAcM,EAENF,mBAAA,cAAiB,EACjBG,UAQM,EAENH,mBAAA,WAAc,EACdI,UAEM,EAENJ,mBAAA,WAAc,EAClBN,mBAAA,CA2BM,OA3BNW,UA2BM,GA1BJX,mBAAA,CAyBO;IAzBAY,QAAM,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,YAAA,IAAAD,QAAA,CAAAC,YAAA,IAAAF,IAAA,CAAY;MACzBR,mBAAA,iBAAoB,EACpBW,UAGM,C"},"metadata":{},"sourceType":"module","externalDependencies":[]}